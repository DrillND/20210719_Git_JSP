<%
	request.setCharacterEncoding("EUC-KR");
	userDTO NewUser = new userDTO();
	if(request.getParameter("userID")!=null)
	{
		NewUser.setUserID(request.getParameter("userID"));
	}
	if(request.getParameter("userPassword")!=null)
	{
		NewUser.setUserID(request.getParameter("userPassword"));
	}
	if(NewUser.getUserID()==null || NewUser.getUserPassword()==null)
	{
		printWriter script = response.getWriter();
		script.printIn("<script>");
		script.printIn("alert('입력 안된것 있다.')");
		script.printIn("</script>");
		script.close();
		return;
		
	}
	
%>
</body>
